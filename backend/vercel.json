{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
        ],
        "routes": [
        {
            "src": "/user/(.*)",
            "dest": "/user/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
}